{% for row in curseur %}
<row id ="{{row.id}}"{{m.locked(isModif and row.isModif)}}{{m.bgColorLevelBloque(row.valide,row.level,row.isModif)}}>
    <cell>{{row.journal_id}}</cell>
    <cell><![CDATA[{{m.lienJournal(row.journal_id,row.msg)}}]]></cell>
    <cell>{{row.groupe_id}}</cell>
    <cell>{{row.jour_id}}</cell>
    <cell>{{row.tournee_id}}</cell>
    <cell><![CDATA[{{m.lienTournee()}}]]></cell>
    <cell>{{row.date_debut|date('d/m/Y')}}</cell>
    <cell>{{row.date_fin|date('d/m/Y')}}</cell>
    <cell>{%if row.employe_id %}<![CDATA[{{m.lienEmploye()}}]]>{% endif %}</cell>
    <cell>{{row.employe_id}}</cell>
    <cell>{%if row.remplacant_id %}<![CDATA[{{m.lienEmploye()}}]]>{% endif %}</cell>
    <cell>{{row.remplacant_id}}</cell>
    <cell>{{row.heure_debut}}</cell>
    <cell>{{row.duree}}</cell>
    <cell>{{row.nbcli}}</cell>
    <cell>{{row.nbkm}}</cell>
    <cell>{{row.nbkm_paye}}</cell>
    <cell>{{row.transport_id}}</cell>
    <cell>{{row.depart_depot}}</cell>
    <cell>{{row.retour_depot}}</cell>
    <cell>{{row.valrem}}</cell>
    <cell>{{row.valrem_moyen}}</cell>
    <cell>{{row.employe_id}}</cell>
    <cell>{{row.valrem}}</cell>
    <cell>{{row.date_debut}}</cell>
    <cell>{{row.tauxhoraire}}</cell>
    <cell>{{isSuperModif}}</cell>
    {% if isSuperModif %} {{m.cellSupprimeRow(isModif,row.isDelete)}} {% endif %}
</row> 
{% endfor %}
