{% for row in curseur %}
<row id ="{{row.id}}"{{m.locked(isModif)}}{{m.bgColorLevel(row.valide,row.level)}}>  
    <cell>{{row.journal_id}}</cell>
    <cell><![CDATA[{{m.lienJournal(row.journal_id,row.msg)}}]]></cell>
    <cell>{{row.tournee_id}}</cell>
    <cell>{{row.activite_id}}</cell>
    <cell>{{row.date_distrib}}</cell>
    <cell>{{row.jour_id}}</cell>
    <cell>{{row.libelle}}</cell>
    <cell>{{row.nbkm}}</cell>
    <cell>{{row.nbkm_paye}}</cell>
    <cell>{{row.transport_id}}</cell>
    <cell>{{row.heure_debut}}</cell>
    <cell>{{row.duree_totale}}</cell> 
    <cell>{{row.duree_nuit}}</cell> 
    <cell>{{row.duree_tournee}}</cell> 
    <cell>{{row.duree_activite}}</cell> 
    <cell>{{row.duree_supplement}}</cell> 
    <cell>{{row.duree_reperage}}</cell> 
    <cell>{{row.nbcli}}</cell>
    <cell>{{row.nbtitre}}</cell>
    <cell>{{row.nbadr}}</cell>
    <cell>{{row.nbspl}}</cell>
    <cell>{{row.nbprod}}</cell>
    <cell>{{row.nbrep}}</cell>
    <cell>{{row.commentaire}}</cell>
</row> 
{% endfor %}
