<script>
function subst() {
  var vars={};
  var x=document.location.search.substring(1).split('&');
  for(var i in x) {var z=x[i].split('=',2);vars[z[0]] = unescape(z[1]);}
  var x=['frompage','topage','page','webpage','section','subsection','subsubsection'];
  for(var i in x) {
    var y = document.getElementsByClassName(x[i]);
    for(var j=0; j<y.length; ++j) y[j].textContent = vars[x[i]];
  }
}
</script></head><body style="border:0; margin: 0;" onload="subst()">
<table style="border-bottom: 1px solid black; width: 100%">
  <tr>
  	<td style="width:50%;text-align:left;"> {{ depot }} </td>
  	<td style="width:30%;text-align:left;"><span class="libelle">Tournee :</span> {{ tournee }}</td>
    <td style="text-align:right;">
      <span class="libelle">Page</span> <span class="page"></span> <span class="libelle">sur</span> <span class="topage"></span>
    </td>
  </tr>
</table>
</body></html>