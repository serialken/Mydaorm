{% import 'AmsModeleBundle:DhtmlxGrid:macros.html.twig' as m %}
<rows>
    <head>      
        <column id="code"               type="ed"   width="70"  align="left"    sort="str">Code</column>
        <column id="libelle"            type="ed"   width="*"   align="left"    sort="str">Libellé</column>
        <column id="typeurssaf_id"      type="coro" width="150" align="left"    sort="sortCombo">Type Urssaf{{m.combo(comboTypeUrssaf)}}</column> 
        <column id="duree_livraison"    type="ed"   width="70"  align="center"  sort="str">Durée de livraison</column> 
    </head> 
    {% for row in curseur %}
        <row id ="{{row.id}}"{{m.locked(isModif)}}>  
            <cell>{{row.code}}</cell>
            <cell>{{row.libelle}}</cell>
            <cell>{{row.typeurssaf_id}}</cell>
            <cell>{{row.duree_livraison}}</cell>
        </row> 
     {% endfor %}
 </rows>