{% extends app.request.xmlHttpRequest 
			? 'AmsSilogBundle::gabarit_base_ajax.html.twig'
			: 'AmsSilogBundle::gabarit_base.html.twig' %}	

{% block css %}
        {{ parent() }}
        <link rel="stylesheet" type="text/css" href="{{asset ('js/dhtmlxSuite/dhtmlx_pro_full/dhtmlx.css') }}">
        <style TYPE="text/css">
        div.gridbox_dhx_skyblue .dhtmlx_invalid{background-color:#FAD9DD}
        div.gridbox_dhx_skyblue .dhtmlx_ecart10{background-color:#F4FA58}
        </style>
{% endblock %}   

{% block js %}
    {{ parent() }}
    <script src="{{ asset('js/fullcalendar-2.0.2/lib/moment.min.js') }}" type="text/javascript" /> </script>

    <script src="{{ asset('js/dhtmlxSuite/dhtmlx_pro_full/dhtmlx.js') }}" type="text/javascript" /> </script>
<!--        <script src="/MRoad/web/js/dhtmlxSuite/dhtmlxDataProcessor/codebase/dhtmlxdataprocessor_debug.js" type="text/javascript" /> </script>-->
    <script  src="{{ asset('js/dhtmlxSuite/dhtmlxGrid/codebase/ext/dhtmlxgrid_export.js') }}"  type="text/javascript" /> </script>   
    <script  src="{{ asset('js/dhtmlxSuite/dhtmlxGrid/codebase/ext/dhtmlxgrid_undo.js') }}"  type="text/javascript" /> </script>   
   {# verification des champs saisis #}
   <script  src="{{ asset('js/dhtmlxUtilities.js')}}" type="text/javascript" /> </script>     
   <script  src="{{ asset('js/dhtmlxVerificator.js')}}" type="text/javascript" /> </script> 
   {# pour les msg flash #}
   <script  src="{{ asset('js/msroad_fonctions.js')}}" type="text/javascript" /> </script> 
{% endblock %}

{% block title %} {% endblock %}

{% block body %}
<div class="container-fluid">
    <div class="row">	
        <div class="col-sm-3 col-md-2 sidebar" id="layout_sidebar">
            {% block sidebar %} {% endblock %}
        </div>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main" style="margin-left:0px" id="layout_dashboard">
            {% block dashboard %} {% endblock %}
        </div>
    </div>
</div>
{% endblock %}

{% block modalTitre %} Hello World {% endblock %}
