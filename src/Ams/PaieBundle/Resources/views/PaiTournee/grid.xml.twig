{% import 'AmsPaieBundle:DhtmlxGrid:macros.html.twig' as m %}
<rows>
    <head>      
        <column id="tournee_mere"       type="ro"   hidden="true"></column> 
        <column id="depot_id"           type="ro"   hidden="true"></column>
        <column id="flux_id"            type="ro"   hidden="true"></column> 
        <column id="date_distrib"       type="ro"   hidden="true"></column>
        <column id="groupe_id"          type="coro" width="50"  align="center"  sort="sortCombo">Groupe{{comboGroupe|raw}}</column>
        <column id="tournee_id"         type="coro" width="200" align="left"    sort="sortCombo">Tournée{{comboTournee|raw}}</column>
        <column id="journal_id"         type="ro"   hidden="true"></column>
        <column id="lien_journal"       type="ro"   width="30"  align="center"  sort="str"></column> 
        <column id="lien_planning"      type="ro"   width="30"  align="center"  sort="na"></column> 
        <column id="employe_id"         type="coro" width="200" align="left"    sort="sortCombo">Employé *{{comboEmploye|raw}}</column> 
        <column id="nbkm"               type="ed"   width="50"  align="right"   sort="int">Transport *</column> 
        <column id="nbkm_paye"          type="ed"   width="50"  align="right"   sort="int">#cspan</column> 
        <column id="transport_id"       type="coro" width="80"  align="center"  sort="sortCombo">#cspan{{comboTransport|raw}}</column> 
        <column id="valrem_org"         type="ron"  width="50"  align="center"  sort="int">Valeur de rémunération</column> 
        <column id="valrem"             type="ron"  width="70"  align="center"  sort="int">#cspan</column> 
        <column id="valrem_calculee"    type="ron"  width="70"  align="center"  sort="int">#cspan</column> 
        <column id="ecart"              type="ro"   width="40"  align="right"   sort="int">#cspan</column> 
        <column id="majoration"         type="coro" width="80"  align="center"  sort="sortCombo">#cspan<option value="0.00">0</option><option value="10.00">10</option><option value="50.00">50</option></column> 
        <column id="duree_retard"       type="ed"   width="50"  align="center"  sort="str">Temps</column> 
        <column id="duree_attente"      type="ed"   width="50"  align="center"  sort="str">#cspan</column> 
        <column id="heure_debut"        type="ed"   width="50"  align="center"  sort="str">Heure de début</column> 
        <column id="heure_debut_calculee" type="ro" width="70"  align="center"  sort="str">#cspan</column> 
        <column id="duree_totale"       type="ro"   width="70"  align="center"  sort="str">Durée</column> 
        <column id="duree_nuit"         type="ro"   width="70"  align="center"  sort="str">#cspan</column> 
        <column id="duree_tournee"      type="ro"   width="70"  align="center"  sort="str">#cspan</column> 
        <column id="duree_supplement"   type="ro"   width="70"  align="center"  sort="str">#cspan</column> 
        <column id="duree_reperage"     type="ro"   width="70"  align="center"  sort="str">#cspan</column> 
        <column id="nbcli"              type="ro"   width="70"  align="right"   sort="int">Quantité</column> 
        <column id="nbtitre"            type="ro"   width="70"  align="right"   sort="int">#cspan</column> 
        <column id="nbadr"              type="ro"   width="70"  align="right"   sort="int">#cspan</column> 
        <column id="nbspl"              type="ro"   width="70"  align="right"   sort="int">#cspan</column> 
        <column id="nbprod"             type="ro"   width="70"  align="right"   sort="int">#cspan</column> 
        <column id="nbrep"              type="ro"   width="70"  align="right"   sort="int">#cspan</column> 
        <column id="poids"              type="ron"  width="70"  align="right"   sort="int" format="0.000">Poids en kg</column> 
        <column id="old_employe_id"     type="ro"   hidden="true"></column>
        {{m.columnSansAjout(isModif and isAlim)}}
	<beforeInit> 
            <call command="attachFooter"><param>,,,,,,Total,,,,#stat_total,#stat_total,,,,,,,,,,,,,,,,#stat_total,#stat_total,#stat_total,#stat_total,#stat_total,#stat_total,#stat_total,{%if isModif and isAlim%},{%endif%}</param></call>
        </beforeInit> 
        <afterInit> 
            <call command="splitAt"><param>6</param></call>   
        </afterInit> 
   </head>      
   {% include 'AmsPaieBundle:PaiTournee:row.xml.twig' %}
</rows>