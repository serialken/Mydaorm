{% for row in curseur %}
<row id ="{{row.id}}"{{m.locked(isModif)}}{{m.bgColorLevel(row.valide,row.level)}}>  
    <cell>{{row.date_debut}}</cell>
    <cell>{{row.depot_id}}</cell>
    <cell>{{row.flux_id}}</cell>
    <cell>{% if row.valide %}<![CDATA[{{m.lienAnnexeEtalon()}}]]>{% endif %}</cell>
    <cell><![CDATA[{{m.lienRemplacementJour()}}]]></cell>
    <cell>{{row.contrattype_id}}</cell>    
    <cell>{{row.remplace_id}}</cell>    
    <cell>{{row.actif}}</cell>
    <cell>{{row.journal_id}}</cell>
    <cell><![CDATA[{{m.lienJournal(row.journal_id,row.msg)}}]]></cell>
    <cell>{{row.date_debut|date('d/m/Y')}}</cell>
    <cell>{{row.date_fin_prevue|date('d/m/Y')}}</cell>
    <cell>{{row.date_fin|date('d/m/Y')}}</cell>
    <cell>{{row.valrem_moyen_semaine}}</cell>
    <cell>{{row.valrem_moyen_dimanche}}</cell>
    <cell>{{row.cycle}}</cell>
    <cell{% if not row.lundi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_lundi_id}}</cell>
    <cell{% if not row.mardi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_mardi_id}}</cell>
    <cell{% if not row.mercredi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_mercredi_id}}</cell>
    <cell{% if not row.jeudi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_jeudi_id}}</cell>
    <cell{% if not row.vendredi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_vendredi_id}}</cell>
    <cell{% if not row.samedi %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_samedi_id}}</cell>
    <cell{% if not row.dimanche %} locked="true" class="dh_cell_bg_grey"{% endif %}>{{row.tournee_dimanche_id}}</cell>
    {{m.cellSupprimeRow(isModif,row.isDelete)}}
</row> 
{% endfor %}
