{% extends '::base.html.twig' %}	
{% block css %}
        {{ parent() }}
        {% stylesheets filter='cssrewrite' output='css/compresse/core.css' 
            'js/fullcalendar-2.7.2/fullcalendar.css'
        %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" media="screen" />
        {% endstylesheets %}
{% endblock %}
{% block js %}
        {% javascripts output='js/compresse/core.js'
                'js/jquery-1.10.2.min.js' 
                'js/jquery.blueimp-gallery.min.js'
                'js/bootstrap.js'
                'js/msroad_fonctions.js'
                'js/fullcalendar-2.7.2/lib/*.js'
                'js/fullcalendar-2.7.2/fullcalendar.js'
                'js/fullcalendar-2.7.2/lang/fr.js'
                'js/fullcalendar-2.7.2/gcal.js'
        %}
            <script src="{{ asset_url }}"></script>
            <script  src="{{ asset('js/dhtmlxUtilities.js')}}" type="text/javascript" /> </script>     
        {% endjavascripts %}

{% endblock %}