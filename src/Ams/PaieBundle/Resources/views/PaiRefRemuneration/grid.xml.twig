{% import 'AmsPaieBundle:DhtmlxGrid:macros.html.twig' as m %}
<rows>
    <head>      
        <column id="societe_id"     type="coro"         width="*"   align="left"    sort="sortCombo">Société{{m.combo(comboSociete)}}</column> 
        <column id="population_id"  type="coro"         width="*"   align="left"    sort="sortCombo">Population{{m.combo(comboPopulation)}}</column> 
        <column id="date_debut"     type="dhxCalendar"  width="70"  align="center"  sort="date">Début</column> 
        <column id="date_fin"       type="dhxCalendar"  width="70"  align="center"  sort="date">Fin</column> 
        <column id="valeur"         type="ro"           width="80"  align="right"   sort="int">Valeur</column> 
        <column id="valeurHP"       type="ro"           width="80"  align="right"   sort="int">Valeur HP</column> 
        <column id="valeurHP2"      type="ro"           width="80"  align="right"   sort="int">Valeur HP Polyvalent</column> 
    </head> 
    {% for row in curseur %}
        <row id ="{{row.id}}"{{m.locked(false)}}>  
            <cell>{{row.societe_id}}</cell>
            <cell>{{row.population_id}}</cell>
            <cell>{{row.date_debut|date('d/m/Y')}}</cell>
            <cell>{{row.date_fin|date('d/m/Y')}}</cell>
            <cell>{{row.valeur}}</cell>
            <cell>{{row.valeurHP}}</cell>
            <cell>{{row.valeurHP2}}</cell>
        </row> 
     {% endfor %}
 </rows>




