{% extends 'AmsSilogBundle::gabarit_base.html.twig' %}

{% block title %}{{titre}}{% endblock %}
{% block pagetitle %}> <a href="{{ path('_ams_accueil_defaut') }}" class="level0">Accueil</a> > {{libCategorie}} 
    <SPAN style="float: right; position:relative"> <a href= "{{pageWiki}}" target="_blank"> Aide?</a> </SPAN>
{% endblock %}

{% block body %}
    {%for sousCategorie in listeSousCategories %} 
        <div class="bloc interlink">
            <table cellspacing="0" cellpadding="0" width="100%">
                <tr>
                    <td rowspan="2" class="border"><img src="{{ asset('images/trans.png') }}" width="10" height="1"></td>
                    <td class="content"><a href="{{path(sousCategorie.page_defaut)}}"><img class="{{sousCategorie.class}}" src="{{ asset('images/trans.png') }}"></a></td>
                </tr>
                <tr>
                    <td class="title"><a href="{{path(sousCategorie.page_defaut)}}">{{sousCategorie.libelle}} </a></td>
                </tr>
            </table>
        </div>		
    {% endfor %}
   
{% endblock %}

     
 
 
 
 